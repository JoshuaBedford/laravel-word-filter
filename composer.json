{
    "name": "joshuabedford/laravel-word-filter",
    "description": "A word and/or profanity filter for Laravel.",
    "keywords": [
        "laravel",
        "profanity",
        "word",
        "filter",
        "validate",
        "validation"
    ],
    "homepage": "https://github.com/JoshuaBedford/laravel-word-filter",
    "license": "MIT",
    "authors": [
        {
            "name": "Joshua Bedford",
            "homepage": "https://joshuabedford.com",
            "role": "Developer"
        }
    ],
    "type": "project",
    "require": {
        "php": ">=5.5.9"
    },
    "require-dev": {
        "laravel/laravel": "5.*",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*",
        "phpunit/phpunit": "^7.5",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "JoshuaBedford\\LaravelWordFilter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JoshuaBedford\\LaravelWordFilter\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JoshuaBedford\\LaravelWordFilter\\WordFilterServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
